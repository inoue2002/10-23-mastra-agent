{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": ".mastra/output/index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [".mastra/output/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".mastra/output/index.mjs"
    }
  ]
}
